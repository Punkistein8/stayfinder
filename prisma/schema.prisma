generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_URI")
}

model hoteles {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  estrellas   String
  foto        String
  informacion String[]
  nombreHotel String
  precio      String
}

model hotelesPrueba {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  estrellas   String
  foto        String
  informacion String[]
  nombreHotel String
  precio      String
}
